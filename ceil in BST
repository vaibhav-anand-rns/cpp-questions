int floorinBST(Treenode <integer> root, int key){
    int ceil = -1;
    while(root){
        if(root -> val == key){
            ceil = root -> val;
            return ceil;
        }
        if(key > root -> val){
            ceil = root->val;
            root = root->right;
            
        }
        else{
            root = root->left;
        }
        return ceil;
}
