struct node{
    
    int data;
    node->next;
    node(int val){
        
        data = val;
        next = nullptr;
    }
}
void deleteMiddleNode(Node*& head){
    
    if(head == nullptr){
        
        delete head;
        head = nullptr;
        return ;
    }
    
    node*slow = head;
    node*fast = head;
    mode*prev = nullptr;
    
    while(fast && fast->next){
         fast = fast->next->next;
         slow = slow->next;
         prev = slow;
    }
    prev->next = slow->next;
    delete slow;
}
