struct node{
    node->next;
    int data;
    node(int val){
        
        data = val;
        next = nullptr;
    }
}
bool detectlopp(node*head){
    
    node*slow = head;
    node*fast = head;
    
    while(fast && fast->next){
        
        slow = slow->next;
        fast = fast->next->next;
        
    }
    if(slow == fast){
        
        return true;
    }
    
    return false;
}
