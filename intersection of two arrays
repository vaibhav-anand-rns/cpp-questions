vector<int> find Arrayintersection(vector<int> &A, int &B){
    int i = 0;
    int j = 0;
    vector<int> ans;
    while(i<n && j<m){
        if(A[i] < B[j]){
            i++;
        }
        else if(B[j]< A[i]){
            j++;
            
        }
        else{
            ans.push_back(A[i]);
            i++;
            j++;
        }
    }
    return ans;
}
