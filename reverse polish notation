#include <iostream>
#include <stack>
#include <vector>
#include <string>
using namespace std;

int evalRPN(vector<string>& tokens) {
    stack<int> st;

    for (string token : tokens) {
        // If token is an operator
        if (token == "+" || token == "-" || token == "*" || token == "/") {
            int b = st.top(); st.pop();
            int a = st.top(); st.pop();
            int result = 0;

            if (token == "+") result = a + b;
            else if (token == "-") result = a - b;
            else if (token == "*") result = a * b;
            else if (token == "/") result = a / b;

            st.push(result);
        } else {
            // It's a number
            st.push(stoi(token));
        }
    }

    return st.top(); // Final result
}

int main() {
    vector<string> tokens = {"2", "1", "+", "3", "*"};  // Example input
    int result = evalRPN(tokens);
    cout << "Result: " << result << endl;  // Output: 9
    return 0;
}
