class solution{
    public:
           void reverse array(int s, int e){
               int s = 0;
               int e = n-1;
               while(s<e){
                   swap(arr[s],arr[e]);
                   s++;
                   e--;
               }
               for(int i=0; i<n; i++){
                   cout<<arr[i]<<" ";
               }
               return 0;
           }
}
