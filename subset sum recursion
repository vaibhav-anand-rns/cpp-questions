class solution{
    public:
           void func(int sum, int ind, vector<int> &arr, int N, vector<int> &sumSubset){
               if(index == N){
                   sumSubset.push_back(sum);
                   return;
               }
               func(ind+1, sum + arr[ind], arr, N, sumSubset);
               func(ind+1, sum, arr, N , sumSubset);
           }
    
           
    
}
